.container {
  width: 100%; 
  padding: 0;
}
.text {
  text-align: center;
}
.schoolName {
  font-size: 60rpx;
}
.imgContainer {
  position: relative; 
  width: 100%;
}
.wrap {
  margin: 0 auto;
  width: 90%;
  height: 650rpx;
  padding-top: 20rpx;
}
.imgWrap {
  display: flex;
  justify-content: center;
}

.schoolImg {
  border: 1px solid gray;
  flex:1;
  height: 200rpx;
}
.checkBoxContainer {
  width: 90%;
  height: 420px;
  margin-top:10px; 
  position: relative;
  display: flex;
  justify-content: center;
}
#checkBox {
  width:95%;
  height: inherit;
}
#checkBox image {
  width: 100%; 
  height: 420px;
}
#box {
  width: 99%;
  height: 300px;
  border: 1px solid red;
}
button {
  text-align: center;
}
#ShowChooseImg view {
  margin: 0 auto;
}
#ShowChooseImg image {
  width:160px;
  height: 160px;
}
#ShowChooseImg .sscontainer {
  position:relative;
  width:160px;
  height: 160px;
  border: 1px solid gainsboro;
  background-size: contain; 
}
.bottomImg {
  width: 100%;
  height: auto;
  margin: 0 auto;
  margin-top:10px; 
}
.bottomImg image{
 width: 100%;
 height: 150px;
}


#asd image {
  z-index: -1;
  border-radius: 50%;
  width: 118px;
  height: 118px; 
  position: absolute;
  top:50%;
  left: 50%;
  margin-top: -59px;
  margin-left: -59px;
}

.canvasContainer {
  margin: 0 auto;
  margin-top:10px; 
}
.copyRight {
  width:90%;
  margin: 0 auto;
  margin-top: 20px;
  font-size: 12px;
  color:dimgray;
}
